		</tr>	
		<tr>	
			<td whidt="40px" colspan="3" class="xls_align_left">Posicion del <%=GF_FN2DTE(db2FechaActual)%> 00:00 Horas</td>
		</tr>	
	</table>	
<%
	Call armarSaldos(pto, rsSalIni, fechaMenos, cdCliente)
end if
'---------------------------------------DESCARGA CAMIONES-----------------------------------------------
Set rsCamDes  = ObtenerSQLDescargaCamiones(fechaActual, pto, cdCliente)
if(not rsCamDes.eof)then
	Set rsColumnas	= armarColumnaClientesCamiones(fechaActual,pto,"TF.cdproducto,dsproducto,TF.dtcontable",cdCliente)
	Set rsFilas		= armarColumnaClientesCamiones(fechaActual,pto, "TF.cdcliente,dscliente,TF.dtcontable",cdCliente)
	Call armarSeccionDescargas(pto, rsCamDes, rsColumnas, rsFilas, FechaFormalA, "Descargas de Camiones del " & GF_FN2DTE(db2FechaActual), "Kilos", "Camiones")
end if
'-------------------------------DESCARGA VAGONES-------------------------------------------------------
Set rsVagDes  = ObtenerSQLDescargaVagones(fechaActual, pto, cdCliente)
if(not rsVagDes.eof)then
	Set rsColumnas	= ArmarColumnasVagones(fechaActual,pto, "T1.cdproducto, p.dsproducto",cdCliente)
	Set rsFilas		= ArmarColumnasVagones(fechaActual,pto," T1.cdcliente, c.dscliente ",cdCliente)
	Call armarSeccionDescargas(pto, rsVagDes, rsColumnas, rsFilas, FechaFormalA, "Descargas de Vagones del " & GF_FN2DTE(db2FechaActual), "Kilos", "Vagones")
end if
'------------------------------------DESCARGA OLEODUCTO-----------------------------------------------
Set rsOleo = obtenerSQLOleoducto(fechaActual,pto, cdCliente)
if(not rsOleo.eof)then
	Set rsColumnas	= ArmarColumnaOleoducto(fechaActual,pto," m.cdproducto, p.dsproducto ",cdCliente)
	Set rsFilas		= ArmarColumnaOleoducto(fechaActual,pto," m.cdcliente, c.dscliente ",cdCliente)
	Call armarSeccionDescargas(pto, rsOleo, rsColumnas, rsFilas, FechaFormalA, "Oleoductos del " & GF_FN2DTE(db2FechaActual), "Kilos", "Cantidad")
end if
'----------------------------------EMBARCADOS----------------------------------------------
set rsBuques = obtenerSQLEmbarcados(fechaActual,pto,cdCliente)
if not rsBuques.eof then 	
	
	While (not rsBuques.eof) 	
		cdavisoOld = rsBuques("cdaviso")	
		sumTotalKilosDia = 0
		sumTotalKilosSaldo = 0
		sumTotalKilosAcumulado = 0
		sumTotalKilosEstimado = 0
%>
		<table>
			<tr>
				<td  colspan="3" class="xls_align_left"><%="Buque: "&rsBuques("cdbuque")&"-"&rsBuques("dsbuque")%></td>
			</tr>
		</table>
		<table class="xls_border_left" style="width:100%; font-size:9;" >		
			<tr style="background-color:#BABABA; ">				
				<td width="40%" colspan="1" class="xls_align_center">PRODUCTO</td>
				<td width="40%" colspan="1" class="xls_align_center">CLIENTE</td>
				<td width="10%" colspan="1" class="xls_align_center">CARGA ESTIMADA</td>			
				<td width="10%" colspan="1" class="xls_align_center">CARGA DIA</td>			
				<td width="10%" colspan="1" class="xls_align_center">CARGA ACUMULADA</td>									
				<td width="10%" colspan="1" class="xls_align_center">SALDO CARGA</td>
			</tr>			
<%		cdavisoAct = cdavisoOld
		while ((not rsBuques.eof) and (cdavisoOld = cdavisoAct))
			productoOld = rsBuques("cdproducto")
			sumProductoKilosDia = 0
			sumProductoKilosSaldo = 0
			sumProductoKilosAcumulado = 0
			sumProductoKilosEstimado = 0
%>
			<tr><td><% =rsBuques("cdproducto") & "-" & rsBuques("dsproducto") %></td></tr>				
<%			productoAct = productoOld
			while ((not rsBuques.eof) and (cdavisoOld = cdavisoAct) and (productoOld = productoAct))	%>
				<tr>	
					<td></td>
					<td width="40%" colspan="1" align="left"><%=rsBuques("cdcliente")&"-"&rsBuques("dscliente")%></td>
					<% myCargaEstimada = obtenerTopeKilos(rsBuques("cdaviso"),rsBuques("cdproducto"),rsBuques("cdcliente"),pto)%>
					<td width="10%" colspan="1" align="right"><%=myCargaEstimada%></td>					
					<%  myCargaDia = CDbl(rsBuques("kilos"))
						if  (isNull(rsBuques("kilos"))) then myCargaDia=0	%>
					<td width="10%" colspan="1" align="right"><%=myCargaDia%></td>
					<% myCargaAcumulada = obtenerKilosAcumulados(rsBuques("cdaviso"),rsBuques("cdproducto"),pto,rsBuques("cdcliente"), db2FechaActual)%>
					<td width="10%" colspan="1" align="right"><%=myCargaAcumulada%></td>
					<% myCargaSaldo = myCargaEstimada - myCargaAcumulada %>
					<td width="10%" colspan="1" align="right"><%=myCargaSaldo%></td>
				</tr>
<%				sumProductoKilosEstimado	= sumProductoKilosEstimado + myCargaEstimada
				sumProductoKilosDia			= sumProductoKilosDia +	myCargaDia			 
				sumProductoKilosAcumulado	= sumProductoKilosAcumulado	+ myCargaAcumulada
				sumProductoKilosSaldo		= sumProductoKilosSaldo + myCargaSaldo
				rsBuques.MoveNext()
				if (not rsBuques.eof) then
					cdavisoAct = rsBuques("cdaviso")
					productoAct = rsBuques("cdproducto")
				end if
			wend	
			'Sumo los totales generales del buque
			sumTotalKilosEstimado	= sumTotalKilosEstimado + sumProductoKilosEstimado
			sumTotalKilosDia		= sumTotalKilosDia + sumProductoKilosDia			 
			sumTotalKilosAcumulado	= sumTotalKilosAcumulado + sumProductoKilosAcumulado
			sumTotalKilosSaldo		= sumTotalKilosSaldo + sumProductoKilosSaldo
				
			%>
			<tr style="background-color:#EDEDED;" class="xls_align_center">
				<td></td>
				<td style="background-color:#EDEDED; " class="xls_align_center">Subtotal</td>				
				<td class="xls_align_right"><%=sumProductoKilosEstimado%></td>
				<td class="xls_align_right"><%=sumProductoKilosDia%></td>
				<td class="xls_align_right"><%=sumProductoKilosAcumulado%></td>
				<td class="xls_align_right"><%=sumProductoKilosSaldo%></td>
			</tr>	
<%		wend	%>
		<tr><td colspan="6"></td></tr>
		<tr style="background-color:#EDEDED;" class="xls_align_center">
			<td colspan="2" width="40%" style="background-color:#EDEDED; " class="xls_align_center">TOTAL</td>				
			<td width="10%" class="xls_align_right"><%=sumTotalKilosEstimado%></td>
			<td width="10%" class="xls_align_right"><%=sumTotalKilosDia%></td>
			<td width="10%" class="xls_align_right"><%=sumTotalKilosAcumulado%></td>
			<td width="10%" class="xls_align_right"><%=sumTotalKilosSaldo%></td>
		</tr>	
</table>
<br>		
<%	wend	
end if
'-----------------------------------------TRANSFERENCIA------------------------------------------
set rsTransf = obtenerSQLTransferencia(fechaActual,pto,cdCliente) 
if(not rsTransf.eof)then
	Set rsColumnas	= ArmarColumnaTransferencia(fechaActual,pto,"m.cdproducto, dsproducto",cdCliente)
	Set rsFilas		= ArmarColumnaTransferencia(fechaActual, pto, "m.cdcliente, dscliente",cdCliente)
	Call armarSeccionDescargas(pto, rsTransf, rsColumnas, rsFilas, FechaFormalA, "Transferencias del " & GF_FN2DTE(db2FechaActual), "Recibidos", "Transferidos")
end if
'---------------------------------------------RECARGAS-------------------------------------------
Set rsCamRec  = obtenerSQLRecargas(fechaActual, pto,cdCliente)
if(not rsCamRec.eof)then
	Set rsColumnas	= ArmarColumnaRecargas(fechaActual,pto,"T2.cdproducto, T2.dsproducto",cdCliente)
	Set rsFilas		= ArmarColumnaRecargas(fechaActual, pto, "T2.cdcliente, T2.dscliente",cdCliente)
	Call armarSeccionDescargas(pto, rsCamRec, rsColumnas, rsFilas, FechaFormalA, "Recargas de Camiones del " & GF_FN2DTE(db2FechaActual), "Kilos", "Camiones")
end if
'---------------------------------AJUSTES CREDITOS DEBITOS-------------------------------
set rsCtoDto = obtenerSQLAjusteCredDebi(fechaActual, pto, cdCliente)
if(not rsCtoDto.eof)then
	Set rsColumnas	= ArmarColumnaAjustes(fechaActual,pto," cd.cdproducto,p.dsproducto ",cdCliente)
	Set rsFilas		= ArmarColumnaAjustes(fechaActual,pto," cd.cdcliente,c.dscliente ",cdCliente	)
	Call armarSeccionDescargas(pto, rsCtoDto, rsColumnas, rsFilas, FechaFormalA, "Ajustes Creditos Debitos del " & GF_FN2DTE(db2FechaActual), "Debito", "Credito")
end if
'----------------------------AJUSTE STOCK------------------------------------------------------
set rsAjuStock = obtenerSQLAjusteStock(fechaActual, pto,cdCliente)
if(not rsAjuStock.eof)then
%>
<table>
	<tr>	
		<td colspan="2" class="xls_align_left">Ajustes de Stock del <%=GF_FN2DTE(db2FechaActual)%></td>
	</tr>	
</table>
<table style="width:80%; font-size:9;" class="xls_border_left" >
	<tr style="background-color:#BABABA;" whidt="200%">			 
		 <td whidt="60px" colspan="1" class="xls_align_center">CLIENTE</td>				 
		 <% 		
			z =0
			filtroProducto = " a.cdproducto,dsproducto "
			filtroCliente =  " a.cdcliente,dscliente "
			Set rsColumnas = ArmarColumnaAjusteStock(fechaActual,pto,filtroProducto,cdCliente)
			if (not rsColumnas.eof) then
				Redim vectCol(rsColumnas.recordcount)
				Redim  vecTotalesProducto(rsColumnas.recordcount)
			end if
			while(not rsColumnas.eof)%>
				<td  colspan="1" class="xls_align_center"><%=rsColumnas("dsproducto")%></td>
				<%  vectCol(z)= rsColumnas("cdproducto")
					z = z + 1
					rsColumnas.movenext
			wend
		%>
	</tr>		
	    <%
		Set rsFilas = ArmarColumnaAjusteStock(fechaActual,pto,filtroCliente,cdCliente)		
		while(not rsFilas.eof)%>
			<tr>			
				<td  align="left"><%=rsFilas("cdcliente") & " - " & rsFilas("dscliente")%></td>
				<%					 
				 seguir= true	
				 i = 0				 
				 while((not rsAjuStock.eof) and seguir)					
					if(cdbl(rsFilas("cdcliente")) = cdbl(rsAjuStock("cdcliente")))then 				
						if (cdbl(vectCol(i)) = cdbl(rsAjuStock("cdproducto")))then%>
							<td align="right" ><%=cdbl(rsAjuStock("Kilos"))%></td>
							<%
							vecTotalesProducto(i) = cdbl(vecTotalesProducto(i)) + cdbl(rsAjuStock("Kilos"))
							rsAjuStock.MoveNext()
							i = i+1
						else								
							%>
							<td align="right" ></td>
							<%
							i = i+1								
							if (i > UBound(vectCol)) then	seguir = false
						end if
					else					
						seguir = false
					end if					
				wend
				rsFilas.MoveNext()	%>
			</tr>				
		<% wend%>		
			<tr style="background-color:#EDEDED; " class="xls_align_center">
				<td colspan="1" class="xls_align_center">TOTAL</td>														
			<%for i = LBound(vecTotalesProducto) to UBound(vecTotalesProducto)-1 %>
				<td class="xls_align_right"><%=vecTotalesProducto(i)%></td>
			<%next%>	
			</tr>     
	</table>
<br><br><br>
<%
end if
'---------------------------- SALDO FINAL------------------------------------------------------
Set rsSalIni = ObtenerSQLSaldoInicial(fechaActual, pto, cdCliente)
if(not rsSalIni.eof)then
%>
<table>
	<tr>	
		<td colspan="2" class="xls_align_left">Posicion del <%=GF_FN2DTE(db2FechaActual)%> 24:00 Horas</td>
	</tr>	
</table>	
<%
	Call armarSaldos(pto, rsSalIni, fechaActual, cdCliente)
%>

<%end if%>
</body>
</html>