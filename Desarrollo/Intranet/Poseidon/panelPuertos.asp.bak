Call initTaskAccessInfo(TASK_POS_PANEL_PUERTO, session("DIVISION_PUERTO"))

Call GP_CONFIGURARMOMENTOS()
Call GF_SET_IDIOMA(GF_Parametros7("langSel",0,6))

%>
