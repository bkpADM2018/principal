    <script type="text/javascript">
        function bodyOnLoad() {		            
			<% 	if (isToepfer(session("KCOrganizacion"))) then	%>
				var tb = new Toolbar("toolbar",5, '../../images/');
				tb.addButton("toolbar-dbupdate", "Migrar Datos", "openmigrar();");
