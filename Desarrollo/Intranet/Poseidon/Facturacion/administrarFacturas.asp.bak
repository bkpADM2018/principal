					        <option value="<% =TIPO_TRANSPORTE_VAGON %>" <% if (myCdTransporte = TIPO_TRANSPORTE_VAGON) then response.write "selected"%>> <%=GF_Traducir("VAGONES")%></option>
			            </select>
                </div>
                      
