/* ======================================================================
=====   HOME INTRA CUADROS		 									=====
====================================================================== */
.boxer			{ float:left; width:45%; height:200px; min-width:400px; margin:0 10px 10px 0; padding:20px; }
.boxer:hover	{ background-color:#DBF5BB; }

.boxer_img				{ float:left; width:48%; height:170px; margin-left:2%; }
	.boxer_img span		{ clear:both; float:left; width:100%; margin:10px auto; height:auto; text-align:center; }

.boxer_table		{ float:right; width:40%; height:170px; margin-right:2%; text-align:; }
	.boxer_table h2	{ margin:0 0 10px 0; padding:0; }
	.boxer_table li	{ float:left; width:98%; height:auto; padding:2% 2%; list-style:none; border-bottom:1px solid #bbb; }
	.boxer_table li:last-child	{ border-bottom:none; }

.boxer_btn			{ clear:both; float:left; width:100%; height:30px; border-top:1px solid #bbb; }
	.boxer_btn a	{ float:right; vertical-align:middle; line-height:30px; }
	.boxer_btn a:hover	{ background-color:#DBF5BB; }
	.boxer_btn a img	{ vertical-align:middle; border:none; padding-right:10px; }

/* ====================================================================== */


/* ==========================================================================
   LISTADO
========================================================================== */
.content_list				{ width:100%; height:500px; margin:0; padding:0; font-family:Arial, Helvetica, sans-serif;  }
.content_list li			{ float:left; width:48%; min-width:350px; list-style:none; margin:1% 1%; }
.content_list li a			{ float:left; width:96%; height:auto; padding:2%; }
.content_list li a:hover	{ background:#D7F6B3 !important; }

.content_list li a img		{ float:left; width:50px; height:auto; margin:0 4% 0 0; border:none; }
.content_list li a h3		{ float:left; width:76%; margin:0; padding:0; font-size:14px; color:#2E6B4D; }
.content_list li a p		{ float:left; width:76%; margin:0; padding:0; font-size:11px; color:#666 }
